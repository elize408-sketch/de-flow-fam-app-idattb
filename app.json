
{
  "expo": {
    "name": "Flow Fam",
    "slug": "Flow Fam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9F6F1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nl.flowfam.gezinsapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
        "backgroundColor": "#F9F6F1"
      },
      "edgeToEdgeEnabled": true,
      "package": "nl.flowfam.gezinsapp"
    },
    "web": {
      "favicon": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "natively",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Flow Fam"
}
