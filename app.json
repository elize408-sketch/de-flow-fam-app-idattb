{
  "expo": {
    "name": "Flow Fam",
    "slug": "flowfam",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9F6F1"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nl.flowfam.gezinsapp",
      "deploymentTarget": "16.0",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "Flow Fam",
        "CFBundleName": "FlowFam"
      }
    },

    "android": {
      "package": "nl.flowfam.gezinsapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
        "backgroundColor": "#F9F6F1"
      },
      "edgeToEdgeEnabled": true
    },

    "web": {
      "favicon": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
      "bundler": "metro"
    },

    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0",
            "useFrameworks": "static",
            "flipper": false
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 23
          }
        }
      ]
    ],

    "scheme": "flowfam",

    "experiments": {
      "typedRoutes": true,
      "newArchEnabled": true
    },

"extra": {
  "eas": {
    "projectId": "143b077c-c9bc-49ad-8f27-0180e47a0e1a"
    }
  }
}
