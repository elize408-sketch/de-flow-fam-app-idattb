
{
  "expo": {
    "name": "Flow Fam",
    "slug": "Flow Fam",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9F6F1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nl.flowfam.gezinsapp",
      "deploymentTarget": "16.0",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "Flow Fam",
        "CFBundleName": "FlowFam"
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "nl.flowfam.gezinsapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
        "backgroundColor": "#F9F6F1"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/6f631a75-cdea-4028-adf7-a7ba1b956b8b.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.143b077c-c9bc-49ad-8f27-0180e47a6e1a"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0",
            "useFrameworks": "static",
            "flipper": false
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 23
          }
        }
      ]
    ],
    "scheme": "flowfam",
    "experiments": {
      "typedRoutes": true,
      "newArchEnabled": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "143b077c-c9bc-49ad-8f27-0180e47a6e1a"
      }
    },
    "sdkVersion": "54.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "cli": {
      "appVersionSource": "local"
    }
  },
  "scheme": "flowfam"
}
