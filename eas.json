
{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_NO_DOTENV": "1"
      },
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false,
        "developmentClient": false,
        "image": "latest",
        "resourceClass": "m1-large",
        "credentialsSource": "remote",
        "cache": {
          "disabled": false,
          "key": "flowfam-ios-cache-v1",
          "cacheDefaultPaths": true,
          "customPaths": [
            "ios/Pods"
          ]
        }
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "info@flowfam.nl",
        "ascAppId": "6738992901",
        "appleTeamId": "4KNTRYN57N"
      }
    }
  }
}
