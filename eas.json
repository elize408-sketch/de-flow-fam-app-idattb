{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "ios": {
        "simulator": true
      }
    },

    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "ios": {
        "simulator": false
      }
    },

    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_NO_DOTENV": "1"
      },

      "ios": {
        "buildConfiguration": "Release",
        "simulator": false,
        "developmentClient": false,
        "resourceClass": "m-medium",
        "credentialsSource": "remote"
      },

      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      }
    }
  },

  "submit": {
    "production": {
      "ios": {
        "appleId": "info@flowfam.nl",
        "ascAppId": "6738992901",
        "appleTeamId": "4KNTRYN57N"
      }
    }
  }
}